# List the classes on which properties are being used
#
# @title	Number of instances per property per type
# @tag 		model
# @tag		SPARQL 1.1
# @see 		
# @param	
# 
SELECT DISTINCT ?property ?type (COUNT(?x) AS ?numberOfInstances) WHERE {
  ?x a ?type .
  ?x ?property ?value .
}
GROUP BY ?property ?type
ORDER BY ?property DESC(?numberOfInstances)